{
  "name": "momentum",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run pre-build && ng serve",
    "build": "npm run pre-build && node tools/develop/ng-build.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pre-build": "node tools/develop/pre-build.js && npm run content",
    "content": "node tools/content/build-content.js",
    "gen": "node tools/develop/ng-generate.js",
    "i18n": "ng extract-i18n --format=json --output-path src/locale"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.2",
    "@angular/common": "~14.0.2",
    "@angular/compiler": "~14.0.2",
    "@angular/core": "~14.0.2",
    "@angular/forms": "~14.0.2",
    "@angular/platform-browser": "~14.0.2",
    "@angular/platform-browser-dynamic": "~14.0.2",
    "@angular/router": "~14.0.2",
    "highlight.js": "^11.6.0",
    "install": "^0.13.0",
    "marked": "^4.1.0",
    "mframe": "^1.3.0",
    "momentum-abstract": "latest",
    "momentum-constructor-common": "^1.1.3",
    "npm": "^8.17.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "~14.0.2",
    "@angular/compiler-cli": "~14.0.2",
    "@angular/localize": "~14.0.2",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.4",
    "write": "^2.0.0"
  }
}
